{
  "name": "cypress-plugin-snapshots",
  "version": "2.0.0-rc5",
  "description": "Cypress snapshot functionality for data",
  "homepage": "https://github.com/meinaart/cypress-plugin-snapshots",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/meinaart/cypress-plugin-snapshots.git"
  },
  "types": "./commands.d.ts",
  "files": [
    "*.js",
    "*.md",
    "docs",
    "LICENSE",
    "src"
  ],
  "exports": {
    "./commands": {
      "import": "./commands.js",
      "types": "./commands.d.ts"
    },
    "./plugin": {
      "import": "./plugin.js"
    }
  },
  "engines": {
    "node": ">=8.2.1"
  },
  "bugs": {
    "url": "https://github.com/meinaart/cypress-plugin-snapshots/issues"
  },
  "keywords": [
    "cypress",
    "cypress-io",
    "cypress-plugin"
  ],
  "author": "Meinaart van Straalen",
  "license": "MIT",
  "scripts": {
    "ci:test": "jest & npm --prefix cypress run ci:test",
    "cy:open": "npm --prefix cypress run cy:open",
    "cy:run": "npm --prefix cypress run cy:run",
    "cy:verify": "npm --prefix cypress run cy:verify",
    "lint": "eslint",
    "start": "node cypress/test-server/index.js"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "image-size": "^0.7.2",
    "jimp": "^1.6.0",
    "js-base64": "^2.5.1",
    "lodash-es": "^4.17.21",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.3.3",
    "rimraf": "^6.0.1",
    "sanitize-filename": "^1.6.1",
    "source-map-support": "^0.5.10",
    "unidiff": "1.0.2"
  },
  "peerDependencies": {
    "cypress": "^13.0.0"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "prettier": "3.4.2"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
